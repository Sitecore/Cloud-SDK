{
  "extends": ["plugin:@nx/react-typescript", "next", "next/core-web-vitals", "../../.eslintrc.json"],
  "ignorePatterns": ["!**/*", ".next/**/*"],
  "overrides": [
    {
      "files": ["*.*"],
      "rules": {
        "header/header": "off",
        "import/no-default-export": "off",
        "@typescript-eslint/no-explicit-any": "off",
        "@next/next/no-html-link-for-pages": ["error", "apps/search-next/app"]
      }
    }
  ]
}
