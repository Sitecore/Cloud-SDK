{
  "name": "integration-tests/core-e2e",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "integration-tests/core-e2e/src",
  "projectType": "application",
  "targets": {
    "core-e2e": {
      "executor": "@nx/cypress:cypress",
      "options": {
        "cypressConfig": "integration-tests/core-e2e/cypress.config.ts",
        "devServerTarget": "integration-tests/core-next:serve:development",
        "testingType": "e2e",
        "baseUrl": "http://localhost:4600"
      },
      "configurations": {
        "production": {
          "devServerTarget": "integration-tests/core-next:serve:production"
        }
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["integration-tests/core-e2e/**/*.{js,ts}"]
      }
    }
  },
  "tags": [],
  "implicitDependencies": ["integration-tests/core-next"]
}
