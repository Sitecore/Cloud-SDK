{
  "name": "@sitecore-cloudsdk/personalize",
  "version": "0.0.7",
  "main": "dist/cjs/src/index.js",
  "module": "dist/esm/src/index.js",
  "types": "dist/esm/src/index.d.ts",
  "exports": {
    ".": {
      "types": {
        "require": "./dist/cjs/src/index.d.ts",
        "default": "./dist/esm/src/index.d.ts"
      },
      "import": "./dist/esm/src/index.js",
      "require": "./dist/cjs/src/index.js"
    }
  },
  "dependencies": {
    "@sitecore-cloudsdk/engage-core": "^0.0.14",
    "@sitecore-cloudsdk/engage-utils": "^0.0.14"
  },
  "publishConfig": {
    "registry": "https://npm.sitecore.com/test/"
  },
  "scripts": {
    "build": "npm run build:cjs && npm run build:es",
    "build:cjs": "tsc -b tsconfig.cjs.json",
    "build:es": "tsc -b ."
  },
  "files": [
    "dist",
    "README.md"
  ]
}
