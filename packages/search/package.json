{
  "name": "@sitecore-cloudsdk/search",
  "version": "0.5.6-rc.0",
  "license": "Apache-2.0",
  "type": "module",
  "exports": {
    "./server": {
      "import": {
        "types": "./server.d.ts",
        "default": "./server.js"
      },
      "require": {
        "types": "./server.d.ts",
        "default": "./server.cjs"
      }
    },
    "./browser": {
      "import": {
        "types": "./browser.d.ts",
        "default": "./browser.js"
      },
      "require": {
        "types": "./browser.d.ts",
        "default": "./browser.cjs"
      }
    }
  },
  "scripts": {
    "build": "npm run build:cjs && npm run build:es",
    "build:cjs": "tsc -b tsconfig.cjs.json",
    "build:es": "tsc -b ."
  },
  "dependencies": {
    "@sitecore-cloudsdk/core": "^0.5.6-rc.0",
    "@sitecore-cloudsdk/events": "^0.5.6-rc.0",
    "@sitecore-cloudsdk/utils": "^0.5.6-rc.0"
  },
  "files": [
    "dist",
    "README.md",
    "browser.js",
    "browser.cjs",
    "browser.d.ts",
    "server.cjs",
    "server.js",
    "server.d.ts"
  ],
  "engines": {
    "node": ">=18"
  }
}
